{
  "name": "dash-replay",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "node --env-file=.env --import tsx/esm --watch server/index.ts",
    "dev:client": "vite",
    "build": "vite build",
    "start": "NODE_ENV=production node --env-file=.env --import tsx/esm server/index.ts",
    "test": "vitest run"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "@vitejs/plugin-react": "^5.1.4",
    "concurrently": "^9.2.1",
    "hono": "^4.12.1",
    "protobufjs": "^8.0.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "vitest": "^4.0.18"
  }
}
